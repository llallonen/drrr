;\n  will-change: transform;\n  top: 0;\n  height: 80px;\n}\n\n.header {\n  box-shadow: inset 0px -1px 0px #f3f4f4;\n}\n\n.header-inner {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  background: #fff;\n  color: #6e6d7a;\n  height: 80px;\n}\n\n.nav {\n  height: inherit;\n  overflow: visible;\n\n  .links-container {\n    display: none;\n  }\n}\n\n.chevron {\n  float: right;\n  width: 6px;\n  height: 6px;\n  border-right: 1px solid black;\n  border-bottom: 1px solid black;\n  transform: rotate(45deg) translateX(6px);\n}\n\na.burger {\n  width: 60px;\n  text-align: center;\n  margin-left: -30px;\n  line-height: 60px;\n  svg {\n    width: 20px;\n    height: 20px;\n  }\n}\n\n.nav-icon {\n  height: 18px;\n  width: 18px;\n  color: #9e9ea7;\n  position: relative;\n  top: 4px;\n}\n\n.fill-current {\n  fill: currentColor;\n}\n\na.logo {\n  padding-right: 20px;\n  &:hover {\n    opacity: 0.7;\n  }\n}\n\n//auth-group\n.auth-group {\n  .auth-group-item:not(:nth-child(2)) {\n    display: none;\n  }\n}\n\n.nav-search {\n  border: none;\n  background-image: url(/assets/images/search.svg);\n  background-repeat: no-repeat;\n  background-position: 100% 50%;\n  background-size: contain;\n  opacity: 0.7;\n}\n\n.btn {\n  padding: 10px 16px;\n  cursor: pointer;\n  border: none;\n  text-decoration: none;\n  background: #ea4c89;\n  -webkit-transition: all 200ms ease;\n  transition: all 200ms ease;\n  border-radius: 8px;\n  height: 40px;\n  line-height: 20px;\n\n  &:hover {\n    opacity: 0.6;\n  }\n}\n.main-btn {\n  span {\n    font-size: 14px;\n    font-weight: 600;\n    color: white;\n  }\n}\n\n.sign-in {\n  font-size: 16px;\n  color: #9da3a5;\n}\n\n//чет по ширине\n@media (min-width: 920px) {\n  a.burger {\n    display: none;\n  }\n\n  .nav {\n    display: flex;\n    justify-content: space-between;\n    flex-grow: 1;\n    .links-container {\n      display: flex;\n      li.dropdown-item {\n        padding-right: 27px;\n        font-size: 14px;\n        font-weight: 600;\n        align-items: center;\n      }\n    }\n  }\n\n  .auth-group {\n    display: flex;\n    .auth-group-item:not(:nth-child(2)) {\n      display: block;\n      padding-left: 5px;\n    }\n  }\n  .sign-in {\n    margin: 0 13px;\n  }\n}\n\n.nav-dropdown {\n  display: none;\n  background-color: #fff;\n  position: absolute;\n  z-index: 1000;\n  left: 0;\n  top: 80px;\n  width: 100vw;\n  box-shadow: 0px 20px 35px rgba(0, 0, 0, 0.1);\n}\n\n@media (min-width: 920px) {\n  .links-container li:hover .nav-dropdown {\n    display: flex;\n    padding-left: 72px;\n  }\n  .mega-nav-box {\n    width: 180px;\n    padding: 30px 8px;\n    margin-right: 40px;\n\n    .title {\n      font-size: 16px;\n      font-weight: 600;\n      color: #333;\n      padding-top: 15px;\n      padding-bottom: 7px;\n    }\n\n    img {\n      width: 140px;\n      height: 105px;\n      border-radius: 8px;\n      display: block;\n    }\n\n    .mega-description {\n      font-size: 13px;\n      line-height: 18px;\n      font-weight: 400;\n      display: inline-block;\n    }\n  }\n\n  .nav-extras {\n    padding-top: 20px;\n    font-weight: 500;\n\n    ul {\n      flex-direction: column;\n      width: 70%;\n\n      li {\n        padding: 7px 0;\n      }\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"43c331eb953ffae796a7614637cfcadf","cacheData":{"env":{}}}